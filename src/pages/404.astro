---
import "../styles/default.scss";
import "../styles/404.scss";
import Header from '../components/Header.jsx';
import Footer from '../components/footer.jsx';
const email_current="quirozben24@icloud.com";
const phone_current="425-610-5085";
---
<html lang="en">
	<head>
		<title>BW Gutter Installation Services</title>
		<meta charset="utf-8" />
		<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
		<meta name="generator" content={Astro.generator} />
		<meta name="viewport" content="width=device-width" />
		<meta name="theme-color" content="#4F4A3B">
		<meta name="title" content="BW Gutter Installation Services">
		<meta name="description" content="Gutter installation and repair services in Seattle and surrounding area. 
		Call us at (425) 610-5085 or email us to schedule a service today. We offer a variety of services.">
		<meta property="og:title" content="Page Not Found">
		<meta property="og:type" content="website">
		<meta property="og:image" content="../images/og-image.png">
		<meta property="site_name" content="BW Gutter Installation Services">
		<style>
		@import url('https://fonts.googleapis.com/css2?family=Roboto:ital,wght@0,500;0,700;0,900;1,700&display=swap');
		</style>
		<script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ENjdO4Dr2bkBIFxQpeoTz1HIcje39Wm4jDKdf19U8gI4ddQ3GYNS7NTKfAdVQSZe" crossorigin="anonymous"></script>
		</head>
	<body>
	<header>
	<Header email={email_current} phone={phone_current} client:load/>
	</header>
    <div id='status-container'>
        <div id='http'>
    <div id='http-status-code-404'>404</div>
    <div id='http-status-code-404-title'>Page Not Found</div>
    <div id='http-status-code-404-message'>The page you requested is missing or moved.</div>
</div>
</div>
	<footer>
		<Footer/>
	</footer>
	</body>
</html>
